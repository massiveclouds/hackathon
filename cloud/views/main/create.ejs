<form id="ContractForm" role="form" action="/finish">
  <div class="form-group">
    <textarea name="oath" rows="15" cols="100" placeholder="What is your oath"></textarea>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Friend</label>
    <select>
  	   <% for (i=0;i<allUsers.length;i++) { %>
        <option value="<%= allUsers[i].id %>"><%= allUsers[i].get('name') %></option>
      <% } %>
  	</select>
    
  </div>

<div class="row">
  <div class="col-xs-6">Description</div>
  <div class="col-xs-6"><input type="text" class="form-control" placeholder="Oath Description"></div>
</div>

<div class="row">
  <div class="col-xs-6">Checkin Location <br />
  <div style="overflow: visible;" id="map-canvas" />
</div>

<div class="row">
  <div class="col-xs-6">Deadline</div>
  <div class="col-xs-6"><input type="date" class="form-control" placeholder="Oath Description"></div>
</div>

<div class="row">
  <div class="col-xs-6">What happens next?</div>
  <div class="col-xs-6">
  
  <span><input type="checkbox" value="theFacts">Get the Facts Only</input></span>
  
  <span><input type="checkbox" value="donateMoney">Donate 5 bucks</input></div></span>

</div>  
  <button type="submit" class="btn btn-default submit">Submit</button>
</form>















